/* components/tabBar/tabBar.wxss */

.tab-bar {
    position: fixed;
    width: 100%;
    bottom: 0;
    left: 0;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    height: 46px;
    background-color: #abcdef;
    line-height: 46px;
}

.tab-bar-item {
    flex: 0 0 50%;
    text-align: center;
}
